{
  "schemaVersion": 1,
  "id": "syncac",
  "version": "${version}",
  "name": "Syncac",
  "description": "Minecraft auto clicker, synchronizes clicks with Carpet Mod TPS logger in the player list footer.",
  "authors": [
    "TheVoidBlcok"
  ],
  "contact": {
    "sources": "https://github.com/thevoidblock/Syncac",
    "homepage": "https://modrinth.com/mod/Syncac",
    "issues": "https://github.com/thevoidblock/Syncac/issues"
  },
  "license": "GPL-3.0",
  "icon": "assets/syncac/icon.png",
  "environment": "*",
  "entrypoints": {
    "client": [
      "com.thevoidblock.syncac.client.SyncacClient"
    ],
    "main": [
      "com.thevoidblock.syncac.Syncac"
    ],
    "modmenu": [
      "com.thevoidblock.syncac.gui.ModMenuIntegration"
    ]
  },
  "mixins": [
    "syncac.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  }
}
