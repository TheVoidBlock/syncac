{
  "schemaVersion": 1,
  "id": "syncac",
  "version": "${version}",
  "name": "Syncac",
  "description": "Minecraft auto clicker mod, synchronizes clicks with Carpet Mod TPS logger.",
  "authors": [
    "TheVoidBlcok"
  ],
  "contact": {
    "repo": "https://github.com/void/syncac"
  },
  "license": "GPL-3.0",
  "icon": "assets/syncac/icon.png",
  "environment": "*",
  "entrypoints": {
    "client": [
      "com.thevoidblock.syncac.client.SyncacClient"
    ],
    "main": [
      "com.thevoidblock.syncac.Syncac"
    ]
  },
  "mixins": [
    "syncac.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  }
}
